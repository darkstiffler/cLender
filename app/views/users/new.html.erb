<div class="new_user bubble">
    <div class="row indent">
        <h3>Please enter your information below!</h3>
    </div>
    <div class="row">
        <%= form_for @user, html: { autocomplete: :off } do |f| %>
        <p>
        <%= f.label :user_name %>
        <%= f.text_field :user_name %><br>
        <%= f.label :email%>
        <%= f.email_field :email %><br>
        <%= f.label :website %>
        <%= f.text_field :website %><br>
        <%= f.label :born_on %>
        <%= f.date_field :born_on %><br>
        <%= f.label :location %>
        <%= f.text_field :location %><br>
        <br>
        <%= f.submit 'Continue', class: "btn btn-primary pull-right" %>
        </p>

    <% end %>
    </div>
</div>